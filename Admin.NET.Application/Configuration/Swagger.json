{
  "$schema": "https://gitee.com/dotnetchina/Furion/raw/v4/schemas/v4/furion-schema.json",

  "SpecificationDocumentSettings": {
    "DocumentTitle": "排产管理",
    "GroupOpenApiInfos": [
        {
            "Group": "Default",
            "Title": "排产管理",
            "Description": "排产管理",
            "Version": "1.0.0",
            "TermsOfService": ""
        },
        {
            "Group": "All Groups",
            "Title": "所有接口",
            "Description": "排产管理",
            "Version": "1.0.0",
            "TermsOfService": ""
        }
    ],
    "DefaultGroupName": "Default", // 默认分组名
	"RoutePrefix": "apidoc",
	"RouteTemplate": "apidoc/{documentName}.json",
    "DocExpansionState": "List", // List、Full、None
    "EnableAllGroups": true,
    "LoginInfo": {
      "Enabled": true, // 是否开启Swagger登录
      "CheckUrl": "/swagger/checkUrl",
      "SubmitUrl": "/swagger/submitUrl"
    },
    "EnumToNumber": true // 枚举类型生成值类型
  }
}